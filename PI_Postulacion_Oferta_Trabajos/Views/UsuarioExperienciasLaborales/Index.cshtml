@model IEnumerable<PI_Postulacion_Oferta_Trabajos.Models.UsuarioExperienciaLaboral>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UsxEmpresa)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UsxArea)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UsxPuesto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UsxFechaInicio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UsxFechaFin)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UsxNivelExperiencia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Usu)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UsxEmpresa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UsxArea)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UsxPuesto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UsxFechaInicio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UsxFechaFin)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UsxNivelExperiencia)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Usu.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.UsxId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.UsxId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.UsxId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
