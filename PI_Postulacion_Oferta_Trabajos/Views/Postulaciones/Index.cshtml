@model IEnumerable<PI_Postulacion_Oferta_Trabajos.Models.Postulacion>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PosFechaPostulacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Esp)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ofe)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Usu)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PosFechaPostulacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Esp.EspId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ofe.OfeId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Usu.UsuId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.PosId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.PosId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.PosId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
